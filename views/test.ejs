<!doctype html>
<html lang="en">

<head>
  <title>PasswordKeepR - Your Organization's Password Manager</title>

  <meta charset="UTF-8" >
  <meta http-equiv="X-UA-Compatible" content="IE=edge" >
  <meta name="viewport" content="width=device-width, initial-scale=1.0" >

  <link rel="stylesheet" href="/vendor/normalize-4.1.1.css" />
  <link rel="stylesheet" href="/vendor/border-box.css" />
  <link rel="stylesheet" href="/styles/main.css" />
  <link rel="stylesheet" href="/styles/layout.css" />
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.2/font/bootstrap-icons.min.css">

  <link rel="icon" type="image/png" sizes="32x32" href="/favicon_32x32.png">
  <script src="/vendor/jquery-3.0.0.js"></script>
  <script defer src="/scripts/app.js"></script>
</head>

<body>

    <header class="navbar rounded-top bg-light" style="--bs-bg-opacity: .5;">
      <div class="navbar-brand">
        <h1>PasswordKeepR</h1>
        <small class="text-muted">Your Organization's Password Manager</small>
      </div>
      <navigation>Menus</navigation>
    </header> <!-- Will contain logo, etc -->
    <main class="bg-light" style="--bs-bg-opacity: .5;">
      <!-- container for holding the tweets from the database -->
      <div id="container">

        

        <section id="new-password">
          <article class="password-item-form">
            <form id="form" action="/api/passwords" method="POST">
              <div>
                <label for="website-name">Website:</label>
                <input type="text" id="webName" name="webName">
              </div>
              <div>
                <label for="user-name">User Name:</label>
                <input type="text" id="userName" name="userName">
              </div>
              <div>
                <label for="password">Password:</label>
                <input type="text" id="password" name="password">
              </div>
              <div>
                <label for="url">URL:</label>
                <input type="text" id="url" name="url">
              </div>
              <div>
                <label for="category">Category:</label>
                <input type="text" id="category" name="category">
              </div>
              <div>
                <label for="organization">organization:</label>
                <input type="text" id="organization" name="organization">
              </div>
              <div>
                <button id="submit-button" type="submit">Save</button>
                <button type="reset">Cancel</button>
              </div>
            </form>
          </article>
        </section>

        <section id="password-container">
          
        </section>

      </div>

      </div>
    </main>
    <footer class="rounded-bottom bg-light" style="--bs-bg-opacity: .5;">Copyright 2023 | PasswordKeepR, LLC | Canada</footer>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>

</html>
